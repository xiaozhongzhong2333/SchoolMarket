.weui-navbar__item {
  position: relative;
  display: block;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  padding: 13px 0;
  text-align: center;
  font-size: 5;
}

.weui-navbar__item.weui-bar__item_on {
  color: #1aad19;
}

.weui-navbar__slider {
  position: absolute;
  content: " ";
  left: 0;
  bottom: 0;
  width: 3.5em;
  height: 3px;
  background-color: #1aad19;
  -webkit-transition: -webkit-transform 0.3s;
  transition: -webkit-transform 0.3s;
  transition: transform 0.3s;
  transition: transform 0.3s, -webkit-transform 0.3s;
}

.weui-navbar__title {
  display: inline-block;
  font-size: 28rpx;
  max-width: 8em;
  width: auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
  margin-right: 20rpx;
}
.swiper-container {
  width: 750rpx;
  position: relative;
  background-color: white;
}

.swiper_box {
  width: 100%;
  height: 375rpx; /*562.5rpx;*//**/
  overflow: visible;
}

.swiper_item{
  overflow: visible;
  border-radius: 50%;
}

.slide-image {
  /*swiper-item image */
  width: 100%;
  display: inline-block;
  overflow: hidden;
  height: 375rpx; /*562.5rpx;*/
  border-radius: 15rpx;
  background-color: #fff;
  box-shadow: 0 10rpx 15rpx rgba(0, 0, 0, 0.1);
  position: relative;
}

.swiper-container .dots {
  position: relative;
  bottom: 10rpx;
  margin: 20rpx 30rpx 0 0;
  display: flex;
  justify-content: flex-end;
}

.swiper-container .dots .dot {
  margin: 0 10rpx;
  width: 15rpx;
  height: 15rpx;
  border-radius: 50%;
  background-color: #999;
  transition: all 0.6s;
  opacity: 0.5;
}

.swiper-container .dots .dot.active {
  background-color: red;
  opacity: 1;
}
